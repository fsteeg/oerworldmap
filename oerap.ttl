<https://raw.github.com/acka47/oerworldmap/add-oer-ap/rm.ttl#email>
    a ore:Proxy , rdf:Property ;
    rdfs:comment "In the context of this application profile the property schema:email is used to indicate the email address of a person, organization, project, or service. The literal should conform to this regular expression: [A-Z0-9._%+-]+@[A-Z0-9.-]+.[A-Z]{2,4}"@en ;
    ore:proxyFor schema:email ;
    ore:proxyIn <https://raw.github.com/acka47/oerworldmap/add-oer-ap/rm.ttl#ap> ;
    rdfs:label "Email"@en ;
    rdfs:label "E-Mail"@de ;
    rdfs:domain [
      a owl:Class ;
      owl:oneOf ( schema:Organization schema:Service schema:Person foaf:Project·)
      ] ;
    rdfs:range [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
        [
            xsd:pattern "[A-Z0-9._%+-]+@[A-Z0-9.-]+.[A-Z]{2,4}"
        ]
      )
              ] ;
    ap:minCardinality "0" .


<https://raw.github.com/acka47/oerworldmap/add-oer-ap/rm.ttl#contact>

<https://raw.github.com/acka47/oerworldmap/add-oer-ap/rm.ttl#ap>
    a ore:Aggregation , ap:ApplicationProfile ;
    dct:title "Application profile for OER world map prototype." ;
    dct:creator <http://lobid.org/person/ap> ;
    ore:aggregates (rdf:type  schema:logo  schema:name schema:alternateName schema:description schema:url schema:email oo:contact schema:address schema:streetAddress schema:postalCode schema:addressLocality schema:addressCountry dct:license schema:geo schema:latitude schema:longitude schema:sameAs schema:member schema:memberOf schema:providesService schema:provider marcrel:fnd ) .

<https://raw.github.com/acka47/oerworldmap/add-oer-ap/rm.ttl#type>
    a ore:Proxy , rdf:Property ;
    rdfs:comment "In the context of this application profile the property rdf:type is used to indicate the type of a resource (person, organization, project, or service)."@en ;
    ore:proxyFor rdf:type ;
    ore:proxyIn <https://raw.github.com/acka47/oerworldmap/add-oer-ap/rm.ttl#ap> ;
    rdfs:label "Type"@en ;
    rdfs:label "Typ"@de ;
    rdfs:domain foaf:Agent ;
    rdfs:range [ 
      a owl:Class ;
      owl:oneOf ( schema:Organization schema:Service schema:Person foaf:Project·)
      ] ;
    ap:cardinality "1" .

<https://raw.github.com/acka47/oerworldmap/add-oer-ap/rm.ttl#logo>
    a ore:Proxy , rdf:Property ;
    rdfs:comment "In the context of this application profile the property schema:logo is used to link to a logo of an organization, a project, or a service."@en ;
    ore:proxyFor schema:logo ;
    ore:proxyIn <https://raw.github.com/acka47/oerworldmap/add-oer-ap/rm.ttl#ap> ;
    rdfs:label "Logo"@en ;
    rdfs:label "Logo"@de ;
    rdfs:domain [
      a owl:Class ;
      owl:oneOf ( schema:Organization schema:Service foaf:Project·)
      ] ;
    rdfs:range foaf:Image ;
    ap:minCardinality "0" ;
    ap:maxCardinality "1" .

<https://raw.github.com/acka47/oerworldmap/add-oer-ap/rm.ttl#name>
    a ore:Proxy , rdf:Property ;
    rdfs:comment "In the context of this application profile the property schema:name is used to indicate the name of a person, organization, project, or service."@en ;
    ore:proxyFor schema:name ;
    ore:proxyIn <https://raw.github.com/acka47/oerworldmap/add-oer-ap/rm.ttl#ap> ;
    rdfs:label "Name"@en ;
    rdfs:label "Name"@de ;
    rdfs:domain [
      a owl:Class ;
      owl:oneOf ( schema:Organization schema:Service schema:Person foaf:Project·)
      ] ;
    rdfs:range rdfs:Literal ;
    ap:cardinality "1" .


<https://raw.github.com/acka47/oerworldmap/add-oer-ap/rm.ttl#latitude>
    a ore:Proxy , owl:DatatypeProperty ;
    rdfs:comment "In the context of this application profile the property schema:latitude is used to indicate the latitude of schema:GeoCoordinates. Values should conform to this regular expression: ^(-?[0-9]+(.[0-9]+)?), *(-?[0-9]+(.[0-9]+)?)$ ."@en ;
    ore:proxyFor schema:latitude ;
    ore:proxyIn <https://raw.github.com/acka47/oerworldmap/add-oer-ap/rm.ttl#ap> ;
    rdfs:label "Latitude"@en ;
    rdfs:label "Geographische Breite"@de ;
    rdfs:domain schema:GeoCoordinates ;
    rdfs:range [
      a rdfs:Datatype ;
      owl:onDatatype xsd:string ;
      owl:withRestrictions (
        [
            xsd:pattern "^(-?[0-9]+(.[0-9]+)?), *(-?[0-9]+(.[0-9]+)?)$"
        ]
      )
              ] ;
    ap:cardinality "1" .

